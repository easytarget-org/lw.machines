{
  "*gen_marlin2": {
    "machineLabel": "Generic Marlin2 machine",
    "machineDescription": "Template for machines based on Marlin 2.0.6+ firmware",
    "settings": {
      "firmwareLogo":   "marlin2.png",
      "firmwareURL":    "https://marlinfw.org/",
      "machineWidth":   300,
      "machineHeight":  200,
      "gcodeStart":     "",
      "gcodeMillStart": "",
      "gcodeEnd":       "",
      "gcodeMillEnd":   "",
      "gcodeHoming":    "",
      "gcodeToolOn":    "",
      "gcodeToolOff":   "",
      "gcodeSMaxValue": 1000,
      "gcodeLaserIntensitySeparateLine": false,
      "gcodeGenerator": "default",
      "macros": {
        "*GotoXY0": {
          "label": "Goto XY zero",
          "gcode": "G0 X0Y0",
          "keybinding": "ctrl+f1"
        },
        "*ToolOff": {
          "label": "LASER OFF",
          "gcode": "M5",
          "keybinding": "ctrl+f2"
        }
      }
    }
  }
}
