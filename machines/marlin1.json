{
  "*gen_marlin1": {
    "machineLabel": "Generic Marlin1 machine",
    "machineDescription": "Template for machines based on Marlin 1.x firmware",
    "settings": {
      "firmwareLogo":   "marlin1.png",
      "firmwareURL":    "https://marlinfw.org/",
      "machineWidth":   300,
      "machineHeight":  200,
      "gcodeStart":     "",
      "gcodeMillStart": "",
      "gcodeEnd":       "",
      "gcodeMillEnd":   "",
      "gcodeHoming":    "",
      "gcodeToolOn":    "M3",
      "gcodeToolOff":   "M5",
      "gcodeSMaxValue": 1000,
      "gcodeLaserIntensitySeparateLine": true,
      "gcodeGenerator": "marlin",
      "macros": {
        "*GotoXY0": {
          "label": "Goto XY zero",
          "gcode": "G0 X0Y0",
          "keybinding": "ctrl+f1"
        },
        "*ToolOff": {
          "label": "LASER OFF",
          "gcode": "M5",
          "keybinding": "ctrl+f2"
        }
      }
    }
  }
}
