{
  "*gen_smoothie": {
    "machineLabel": "Generic SMOOTHIE machine",
    "machineDescription": "Use this if you have a Smoothie machine",
    "settings": {
      "firmwareLogo": "smoothie.png",
      "firmwareURL": "https://smoothieware.org/",
      "machineWidth": 300,
      "machineHeight": 200,
      "toolImagePosition": "BL",
      "toolUseNumpad": false,
      "toolUseVideo": false,
      "toolWebcamUrl": "",
      "gcodeStart": "G21         ; Set units to mm\r\nG90         ; Absolute positioning\r\n",
      "gcodeMillStart": "G21         ; Set units to mm\r\nG90         ; Absolute positioning\r\n",
      "gcodeEnd": "M2          ; End\r\n",
      "gcodeMillEnd": "M2          ; End\r\n",
      "gcodeHoming": "",
      "gcodeToolOn": "",
      "gcodeToolOff": "",
      "gcodeSMaxValue": 1,
      "toolGridWidth": 400,
      "toolGridHeight": 300,
      "macros": {
        "*GotoXY0": {
          "label": "Goto XY zero",
          "gcode": "G0 X0Y0",
          "keybinding": "ctrl+f1"
        },
        "*ToolOff": {
          "label": "LASER OFF",
          "gcode": "M5",
          "keybinding": "ctrl+f2"
        }
      }
    }
  }
}
