{
  "*gen_rrf": {
    "machineLabel": "Generic RepRap Machine",
    "machineDescription": "Use this for machines based on RepRap controller firmware",
    "settings": {
      "firmwareLogo": "rrf.png",
      "firmwareURL": "https://github.com/Duet3D/RepRapFirmware/wiki",
      "machineWidth": 300,
      "machineHeight": 200,
      "toolImagePosition": "BL",
      "toolUseNumpad": false,
      "toolUseVideo": false,
      "toolWebcamUrl": "",
      "gcodeStart": "",
      "gcodeMillStart": "",
      "gcodeEnd": "",
      "gcodeMillEnd": "",
      "gcodeHoming": "",
      "gcodeToolOn": "",
      "gcodeToolOff": "",
      "gcodeSMaxValue": 1000,
      "toolGridWidth": 400,
      "toolGridHeight": 300,
      "macros": {
        "*GotoXY0": {
          "label": "Goto XY zero",
          "gcode": "G0 X0Y0",
          "keybinding": "ctrl+f1"
        },
        "*ToolOff": {
          "label": "LASER OFF",
          "gcode": "M5",
          "keybinding": "ctrl+f2"
        }
      }
    }
  }
}
