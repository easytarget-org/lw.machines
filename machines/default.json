{
  "*gen_default": {
    "machineLabel": "Default Generic template",
    "machineDescription": "A generic machine template for you to customize as you wish",
    "settings": {
      "firmwareLogo":   "default.png",
      "firmwareURL":    "https://forum.makerforums.info/c/laserweb-cncweb/",
      "machineWidth":   300,
      "machineHeight":  200,
      "gcodeStart":     "; Put your laser startup code here",
      "gcodeEnd":       "; Put your laser end gcode here",
      "gcodeHoming":    "; eg $H or G28",
      "gcodeSMaxValue": 1000,
      "macros": {
        "*GotoXY0": {
          "label": "Goto XY zero",
          "gcode": "G0 X0Y0",
          "keybinding": "ctrl+f1"
        },
        "*ToolOff": {
          "label": "LASER OFF",
          "gcode": "M5",
          "keybinding": "ctrl+f2"
        }
      }
    }
  }
}
